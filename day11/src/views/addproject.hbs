    <div class="container">
        <form method="post" action="/addproject" >
            <div class="row">
                <div class="col-md-6 mx-auto">
                    <h1 class="h1" style="display: flex;justify-content: center; margin-top: 10%;">ADD MY PROJECT</h1>
                    <div class="form-group">
                         <label for="pName" class="text-label">Project name</label>
                        <input type="text" id="pName" name="title" class="form-control" />
                    </div>
                    <div class="container-date">
                        <div class="row">
                            <div class="col-md-6">
                                <label for="startDate" class="text-label">Start Date</label>
                                <input type="date" id="startDate" name="startDate" class="form-control" />
                            </div>
                            <div class="col-md-6">
                                <label for="endDate" class="text-label">End Date</label>
                                <input type="date" id="endDate" name="endDate" class="form-control" />
                            </div>
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="description">Description</label>
                        <textarea id="description" name="description" class="form-control"></textarea>
                    </div>
                    <label class="text-label mr-3">Technologies</label>
                    <div class="form-group d-flex" style="gap: 5px;">
                        <div class="d-flex flex-column">
                            <div class="form-check">
                                <input class="form-check-input" type="checkbox" value="NodeJs" id="tech1" name="technologies">
                                <label class="form-check-label" for="tech1">Node Js</label>
                            </div>
                            <div class="form-check">
                                <input class="form-check-input" type="checkbox" value="ReactJs" id="tech2" name="technologies">
                                <label class="form-check-label" for="tech2">React Js</label>
                            </div>
                        </div>

                        <div class="d-flex flex-column">
                            <div class="form-check">
                                <input class="form-check-input" type="checkbox" value="NextJs" id="tech3" name="technologies">
                                <label class="form-check-label" for="tech3">Next Js</label>
                            </div>
                            <div class="form-check">
                                <input class="form-check-input" type="checkbox" value="TypeScript" id="tech4" name="technologies">
                                <label class="form-check-label" for="tech4">TypeScript</label>
                            </div>
                        </div>
                    </div>

                    <div class="mb-3">
                        <label for="inputImage" class="form-label">Upload image</label>
                        <div class="input-group">
                            <input type="file" class="form-control" id="attachFile">
                            <label for="attachFile" class="input-group-text">
                                <img src="./assets/image/file (1).png" alt="file" style="width: 20px; height: 20px;">
                            </label>
                        </div>
                        <img id="previewImage" src="#" alt="Preview" style="max-width: 100%; display: none;">
                    </div>

                    <div class="col-12 text-end mt-4">
                        <button class="btn btn-secondary rounded-pill " type="submit"
                            style="width: 20%; background-color: #111727;">Submit </button>
                    </div>
                </div>
            </div>
        </form>
